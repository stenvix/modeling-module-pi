<UserControl x:Class="VirtualLaboratoryPI.Graph.Data.Templates.BlockTemplate"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VirtualLaboratoryPI.Graph.Data.Templates"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="150">
    <Grid>
        <Border BorderBrush="Black" BorderThickness="1" Width="150" Margin="0,0,0,0">
            <Label x:Name="label" Content="Label" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="14"/>
        </Border>
    </Grid>
</UserControl>
